{
    "name": "vinkla/translator",
    "description": "An Eloquent translator for Laravel",
    "keywords": ["eloquent", "language", "laravel", "locale", "multilingual", "translation", "translations", "translator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "phpunit/phpunit": "^4.8 || ^5.1.2"
    },
    "autoload": {
        "psr-4": {
            "Vinkla\\Translator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Translator\\": "tests/"
        },
        "classmap": [
            "tests/database"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
